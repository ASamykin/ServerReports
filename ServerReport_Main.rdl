    <DataSet Name="dsMaint2">
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Failed">
          <DataField>Failed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="JobNames">
          <DataField>JobNames</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>with data as 
(
	select 
		job_id
		,count(1) [failed] 
	from msdb.dbo.sysjobhistory
	where 
		run_status in (0,2,3) 
		and step_id=0  /*check only final step*/
		and run_date=convert(int,convert(varchar(20),getdate(),112)) 
	group by job_id
)
, data2 as 
(
	select 
		c.name [Category]
		,j.name [Job]
		,d.failed
	from msdb.dbo.sysjobs j
	join msdb.dbo.syscategories c on j.category_id = c.category_id
	join data d on j.job_id=d.job_id
) 
select 
	dd.[Category]
	,count(1) [Failed]
	,stuff((select char(10)+'* '+d.job+' '+quotename(convert(varchar(10),d.failed)) from data2 d where d.category=dd.category order by d.job for xml path('')),1,1,'') [JobNames]
from data2 dd
group by [Category]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsMaint3">
      <Fields>
        <Field Name="ErrorNum">
          <DataField>ErrorNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
