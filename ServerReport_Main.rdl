        </Field>
        <Field Name="CachedSizeMB">
          <DataField>CachedSizeMB</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalIOMB">
          <DataField>TotalIOMB</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BufferPoolPercent">
          <DataField>BufferPoolPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="IOPercent">
          <DataField>IOPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CPUTimems">
          <DataField>CPUTimems</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="CPUPercent">
          <DataField>CPUPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>;WITH AggregateBufferPoolUsage as
 (
	SELECT database_id AS [Database]
		,CAST(COUNT(*) * 8 / 1024.0 AS DECIMAL(10, 2)) AS [CachedSize]
	FROM sys.dm_os_buffer_descriptors WITH (NOLOCK)
	WHERE database_id &lt;&gt; 32767 -- ResourceDB
	GROUP BY database_id
	)
, DB_CPU_Stats as
 (
	SELECT pa.DatabaseID AS [Database]
		,SUM(qs.total_worker_time / 1000) AS [CPU_Time_Ms]
	FROM sys.dm_exec_query_stats AS qs WITH (NOLOCK)
	CROSS APPLY (
		SELECT CONVERT(INT, value) AS [DatabaseID]
		FROM sys.dm_exec_plan_attributes(qs.plan_handle)
		WHERE attribute = N'dbid'
		) AS pa
	where pa.DatabaseID&lt;&gt;32767
	GROUP BY DatabaseID
	)
	
, Aggregate_IO_Statistics as
 (
	SELECT database_id AS [Database]
		,CAST(SUM(num_of_bytes_read + num_of_bytes_written) / 1048576 AS DECIMAL(12, 2)) AS io_in_mb
	FROM sys.dm_io_virtual_file_stats(NULL, NULL) AS [DM_IO_STATS]
	GROUP BY database_id
	)

SELECT ROW_NUMBER() OVER (
		ORDER BY [io].io_in_mb DESC
		) AS [IORank]
	,db_name([io].[Database]) [Database]

	,[io].io_in_mb AS [TotalIOMB]
	,CAST([io].io_in_mb / SUM([io].io_in_mb) OVER () * 100.0 AS DECIMAL(5, 2)) AS [IOPercent]

	,isnull(cpu.[CPU_Time_Ms],0) AS [CPUTimems]
	,isnull(CAST(cpu.[CPU_Time_Ms] * 1.0 / SUM(cpu.[CPU_Time_Ms]) OVER () * 100.0 AS DECIMAL(5, 2)),0) AS [CPUPercent]

	,bp.CachedSize AS [CachedSizeMB]
	,CAST(bp.CachedSize / SUM(bp.CachedSize) OVER () * 100.0 AS DECIMAL(5, 2)) AS [BufferPoolPercent]

FROM Aggregate_IO_Statistics io
left join DB_CPU_Stats cpu on [io].[Database]=cpu.[Database]
left join AggregateBufferPoolUsage bp on io.[Database]=bp.[Database]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
