<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">
  <DataSources>
    <DataSource Name="dsMain">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=.\a1;Initial Catalog=master</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:DataSourceID>b363011c-b10a-440f-967e-3accb6ca6766</rd:DataSourceID>
      <rd:SecurityType>Windows</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dsServer">
      <Fields>
        <Field Name="srv">
          <DataField>srv</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ver">
          <DataField>ver</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>select @@servername [srv]
		,convert(varchar(20), getdate(), 106) + ' '+convert(varchar(20), getdate(), 108) + ' (UTC' 
			+ iif(datediff(hh,getutcdate(),getdate())&gt;0,'+','-')+convert(varchar(2),datediff(hh,getutcdate(),getdate()))+')' [dt]
		,@@version [ver]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsHost">
      <Fields>
        <Field Name="pars">
          <DataField>pars</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>DECLARE @Domain VARCHAR(50)
DECLARE @man VARCHAR(50)
DECLARE @ver VARCHAR(50)
DECLARE @systemver VARCHAR(50)

EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
	,@key = 'HARDWARE\DESCRIPTION\System\BIOS'
	,@value_name = 'SystemManufacturer'
	,@value = @man OUTPUT;

EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
	,@key = 'HARDWARE\DESCRIPTION\System\BIOS'
	,@value_name = 'SystemVersion'
	,@value = @ver OUTPUT;

EXEC master..xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
	,@key = 'SYSTEM\ControlSet001\Services\Tcpip\Parameters\'
	,@value_name = 'Domain'
	,@value = @Domain OUTPUT

EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
	,@key = 'SYSTEM\HardwareConfig\Current'
	,@value_name = 'SystemVersion'
	,@value = @systemver OUTPUT;

IF @ver IS NULL
	EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
		,@key = 'SYSTEM\HardwareConfig\Current'
		,@value_name = 'SystemProductName'
		,@value = @ver OUTPUT;

IF @man IS NULL
	EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
		,@key = 'SYSTEM\HardwareConfig\Current'
		,@value_name = 'SystemManufacturer'
		,@value = @man OUTPUT;



WITH data
AS (
	SELECT convert(VARCHAR(50), SERVERPROPERTY('ComputerNamePhysicalNetBIOS')) [HostName]
		,convert(VARCHAR(50), CASE 
				WHEN virtual_machine_type_desc = 'NONE'
					THEN 'Physical'
				WHEN virtual_machine_type_desc = 'HYPERVISOR'
					THEN 'Virtual'
				ELSE virtual_machine_type_desc
				END) [ServerType]
		,convert(VARCHAR(50), coalesce(@Domain, default_domain())) [Domain]
		,convert(VARCHAR(50), @man) [Manufacturer]
		,convert(VARCHAR(50), @ver) [Product]
		,convert(VARCHAR(50), @systemver) [Version]
	FROM sys.dm_os_sys_info -- option (recompile)
	)
SELECT pars
	,val
FROM data d
unpivot(val FOR pars IN (
			[HostName]
			,[Domain]
			,[ServerType]
			,[Manufacturer]
			,[Product]
			,[Version]
			)) unp</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsHost2">
      <Fields>
        <Field Name="pars">
          <DataField>pars</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>DECLARE @cpu_speed_mhz int
DECLARE @proc varchar(50)
EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE',@key = 'HARDWARE\DESCRIPTION\System\CentralProcessor\0',@value_name = '~MHz',@value = @cpu_speed_mhz OUTPUT;
EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE',@key = 'HARDWARE\DESCRIPTION\System\CentralProcessor\0',@value_name = 'ProcessorNameString',@value = @proc OUTPUT;

;WITH data
AS (
	SELECT 
		convert(varchar(50),cpu_count / hyperthread_ratio) [Sockets]
		,convert(varchar(50),cpu_count) [CPUs]
		,convert(varchar(50),ceiling(physical_memory_kb / 1024 / 1024.)) [Memory (GB)]
		,convert(varchar(50),convert(varchar(4),convert(numeric(10,2),@cpu_speed_mhz/1000.))) [CPU Speed]
		,convert(varchar(50),@proc) [CPU Name]
	FROM sys.dm_os_sys_info
	)
SELECT 
	pars
	,val
FROM data d
unpivot(val FOR pars IN (
			[Sockets]
			,[CPUs]
			,[CPU Speed]
			,[CPU Name]
			,[Memory (GB)]
			)) unp</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsHost3">
      <Fields>
        <Field Name="pars">
          <DataField>pars</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="error">
          <DataField>error</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>DECLARE @outval VARCHAR(36);

/* Get power plan if set by group policy [Git Hub Issue #1620] */
EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
	,@key = 'SOFTWARE\Policies\Microsoft\Power\PowerSettings'
	,@value_name = 'ActivePowerScheme'
	,@value = @outval OUTPUT;

IF @outval IS NULL /* If power plan was not set by group policy, get local value [Git Hub Issue #1620]*/
	EXEC master.sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE'
		,@key = 'SYSTEM\CurrentControlSet\Control\Power\User\PowerSchemes'
		,@value_name = 'ActivePowerScheme'
		,@value = @outval OUTPUT;

/*
SELECT 'OS Version' [pars], rtrim(stuff(@@version, 1, patindex('% on %', @@version) + 3, '')) [val]
*/
SELECT 'OS Version' [pars]
	,stuff(stuff(@@version, len(@@version) - 1, len(@@version), ')'), 1, patindex('% on %', @@version) + 3, '') [val]
	,0 [error]

UNION ALL

SELECT 'Power Mode'
	,CASE @outval
		WHEN 'a1841308-3541-4fab-bc81-f71556f20b4a'
			THEN 'Power Saving'
		WHEN '381b4222-f694-41f0-9685-ff5bb260df2e'
			THEN 'Balanced'
		WHEN '8c5e7fda-e8bf-4a96-9a85-a6e23a8c635c'
			THEN 'High Performance'
		WHEN 'e9a42b02-d5df-448d-aa00-03f14749eb61'
			THEN 'Ultimate Performance'
		ELSE 'Unknown'
		END
	,CASE 
		WHEN @outval &lt;&gt; '8c5e7fda-e8bf-4a96-9a85-a6e23a8c635c'
			THEN 1
		ELSE 0
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsSQL1">
      <Fields>
        <Field Name="pars">
          <DataField>pars</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="descr">
          <DataField>descr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>select 'Version' [pars], SERVERPROPERTY('ProductVersion') [val], '' [descr] 
union all
select 'Edition',SERVERPROPERTY('Edition'),''
union all
select 'Collation',SERVERPROPERTY('Collation'),''
union all

select 'Authentication',iif(SERVERPROPERTY('IsIntegratedSecurityOnly')=0,'Mixed','Windows') ,'' -- 1 = windows, 0 = mixed
union all
select 'Installed',convert(varchar(20),min(create_date),106),'' [Installed] from sys.server_principals where name like 'NT AUTHORITY%' 
union all
select 'Data Path',SERVERPROPERTY('InstanceDefaultDataPath'),''
union all
select 'Log Path',SERVERPROPERTY('InstanceDefaultLogPath'),''

</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsSQL2">
      <Fields>
        <Field Name="pars">
          <DataField>pars</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="descr">
          <DataField>descr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>select 
	name [pars]
	,value_in_use [val]
	,description [descr] 
from sys.configurations
where 
	name in ('max degree of parallelism','cost threshold for parallelism','min server memory (MB)','max server memory (MB)','optimize for ad hoc workloads')
union all 
select 'CPU affinity'
		,affinity_type_desc
		,'Specifies the type of server CPU process affinity currently in use. '
from sys.dm_os_sys_info
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsSQL3">
      <Fields>
        <Field Name="pars">
          <DataField>pars</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="descr">
          <DataField>descr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>select 'SQL Service Account' [pars],service_account [val],'SQL Server service account.' [descr] from sys.dm_server_services where filename like '%sqlservr.exe%'
union all
select 'SQL Agent Account',service_account,'SQL Agent service account.' from sys.dm_server_services where filename like '%SQLAGENT.exe%'
union all
select 'IFI',iif(instant_file_initialization_enabled='N','not enabled','enabled'),'Instant File Initialization.' from sys.dm_server_services where filename like '%sqlservr.exe%'
union all
select 'Locked Pages (MB)', convert(varchar(50),locked_page_allocations_kb/1024), 'Specifies amount of data locked in memory by SQL server.'  from sys.dm_os_process_memory  
union all
select 'Connection', auth_scheme +' '+ quotename(net_transport), 'Connection protocol and network transport.' from sys.dm_exec_connections where session_id=@@spid
union all
select 'Server Port', convert(varchar(10),local_tcp_port), 'SQL Server port number.' from sys.dm_exec_connections where session_id=@@spid</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsSQL4">
      <Fields>
        <Field Name="tf">
          <DataField>tf</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="status">
          <DataField>status</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="global">
          <DataField>global</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="session">
          <DataField>session</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>declare @tf table (tf varchar(10),[status] tinyint,[global] tinyint,[session] tinyint)
dbcc traceon(3604)
insert into @tf (tf,[status],[global],[session])
exec ('dbcc tracestatus()')

;with dat as (
	select '00000'[tf],'Source' [description],'http://www.sqlservice.se/updated-microsoft-sql-server-trace-flag-list/'  [link] union all 
