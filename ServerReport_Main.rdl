        </Field>
        <Field Name="physmemttl">
          <DataField>physmemttl</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="physmemfree">
          <DataField>physmemfree</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="pagefilettl">
          <DataField>pagefilettl</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="pagefilefree">
          <DataField>pagefilefree</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="memstate">
          <DataField>memstate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="others">
          <DataField>others</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>;with mem as 
(
	select counter_name
			,cntr_value 
	from sys.dm_os_performance_counters 
	where 
		counter_name in ('Target Server Memory (KB)','Total Server Memory (KB)')
)
,mem_p (TargetServerMemoryKB,TotalServerMemoryKB) as 
(
	select 
		[Target Server Memory (KB)]
		,[Total Server Memory (KB)]
	from mem
	pivot 
	(max(cntr_value) for counter_name in([Target Server Memory (KB)],[Total Server Memory (KB)])) p
)
,os as 
(
	select 
		total_physical_memory_kb 
		,available_physical_memory_kb
		,total_page_file_kb
		,available_page_file_kb
		,system_memory_state_desc
	from sys.dm_os_sys_memory
)
select
	 mem_p.TargetServerMemoryKB/1024			[memorytarget]
	,mem_p.TotalServerMemoryKB/1024			[memorytotal]
	,os.total_physical_memory_kb/1024		[physmemttl]
	,os.available_physical_memory_kb/1024	[physmemfree]
	,os.total_page_file_kb/1024				[pagefilettl]
	,os.available_page_file_kb/1024			[pagefilefree]
	,os.system_memory_state_desc        [memstate]
	,(os.total_physical_memory_kb-mem_p.TotalServerMemoryKB-os.available_physical_memory_kb)/1024 [others]
from mem_p 
cross join os </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
