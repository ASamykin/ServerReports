      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>declare @points table ( volume_mount_point nvarchar(512)	)
declare @drives table (drive varchar(5),MBFree int)
declare @drive_info table (
	volume_mount_point nvarchar(512),
	file_system_type nvarchar(512),
	logical_volume_name nvarchar(512),
	[Total_Size_GB] DECIMAL(18,1),
	[Available_Size_GB] DECIMAL(18,1),
	[Space_Free_Percent] DECIMAL(18,1))


insert into @drives (drive,MBFree)
exec xp_fixeddrives 

update @drives 
set drive=drive+':\'

--select * from @drives 
declare @cmd varchar(2048)

if convert(varchar(20),SERVERPROPERTY('ProductVersion')) not like '10.0.%' 
	begin
		select @cmd=
					'with files(database_id,file_id) as
						(select 
							min(database_id)
							,min(file_id)
						from sys.master_files with (NOLOCK)
						group by 
						left(physical_name,len(physical_name)-charindex(''\'',reverse(physical_name)))
						)
					SELECT distinct
						vs.volume_mount_point
					, vs.file_system_type
					, vs.logical_volume_name
					, CONVERT(DECIMAL(18,1),vs.total_bytes/1024/1024/1024.0) AS [Total_Size_GB]
					, CONVERT(DECIMAL(18,1),vs.available_bytes/1024/1024/1024.0) AS [Available_Size_GB]
					, CAST(CAST(vs.available_bytes AS FLOAT)/ CAST(vs.total_bytes AS FLOAT) AS DECIMAL(18,2)) * 100 AS [Space_Free_Percent] 
					FROM files AS f WITH (NOLOCK)
					CROSS APPLY sys.dm_os_volume_stats(f.database_id, f.[file_id]) AS vs 
					OPTION (RECOMPILE)'
		--select @cmd
		insert into @drive_info([volume_mount_point],[file_system_type],[logical_volume_name],[Total_Size_GB],[Available_Size_GB],[Space_Free_Percent])
		exec (@cmd)
	end 

if convert(varchar(20),SERVERPROPERTY('ProductVersion')) not like '10.0.%' 
	begin
		select @cmd=
					'with files(database_id,file_id) as
						(select 
							min(database_id)
							,min(file_id)
						from sys.master_files with (NOLOCK)
						group by 
						left(physical_name,len(physical_name)-charindex(''\'',reverse(physical_name)))
						)
					select distinct
						vs.volume_mount_point
					from [files] f with (NOLOCK)
					cross apply sys.dm_os_volume_stats(f.database_id, f.[file_id]) vs 
					option (recompile)'
		insert into @points([volume_mount_point])
		exec (@cmd)
	end 
else 
	begin
		insert into @points ([volume_mount_point])
		select left(physical_name,3)
		from sys.master_files 
		group by left(physical_name,3)
	end 



;with data as (
select 
	di.[volume_mount_point] [volume_mount_point]
	,convert(varchar(50),di.[file_system_type]) [file_system_type]
	,convert(varchar(50),di.[logical_volume_name]) [logical_volume_name]
	,convert(varchar(50),di.[Total_Size_GB]) [Total_Size_GB]
	,convert(varchar(50),di.[Available_Size_GB]) [Available_Size_GB]
	,convert(varchar(50),di.[Space_Free_Percent]) [Space_Free_Percent]
	,type_desc+' ('+convert(varchar(10),count(1))+')' [dbfiles]
from @drive_info di
join sys.master_files mf  on patindex(di.volume_mount_point+'%',mf.physical_name collate SQL_Latin1_General_CP1_CS_AS)&gt;0 
group by di.volume_mount_point
	,convert(varchar(50),di.[file_system_type])
	,convert(varchar(50),di.[logical_volume_name]) 
	,convert(varchar(50),di.[Total_Size_GB])
	,convert(varchar(50),di.[Available_Size_GB])
	,convert(varchar(50),di.[Space_Free_Percent])
	,mf.type_desc
)
, data2 as (
select distinct 
	 d1.[volume_mount_point]
	,d1.[file_system_type]
	,d1.[logical_volume_name]
	,d1.[Total_Size_GB]
	,d1.[Available_Size_GB]
	,d1.[Space_Free_Percent]
	,isnull(reverse(stuff(reverse((select d2.dbfiles + ', ' as 'data()' from data d2 where d2.[volume_mount_point]=d1.[volume_mount_point] for xml path('') )),1,2,'')),'') [TypeAmount]
from data d1 
union all
select 
	drive,NULL,NULL,NULL,convert(varchar(20),convert(numeric(10,2),d.MBFree/1024.)),NULL,NULL
from @drives d
where drive not in (select [volume_mount_point] from data)
)
,tab as (
	select 
		vs.volume_mount_point [Drive]
		, sum(num_of_reads) [num_of_reads]
		, sum(io_stall_read_ms) [io_stall_read_ms]
		, sum(num_of_writes) [num_of_writes]
		, sum(io_stall_write_ms) [io_stall_write_ms]
		, sum(num_of_bytes_read) [num_of_bytes_read]
		, sum(num_of_bytes_written) [num_of_bytes_written]
		, sum(io_stall) [io_stall]
      from sys.dm_io_virtual_file_stats(NULL, NULL) as vfs
      join sys.master_files as mf with (NOLOCK) on vfs.database_id = mf.database_id and vfs.[file_id] = mf.[file_id]
	  --cross apply sys.dm_os_volume_stats(mf.database_id, mf.[file_id]) as vs /*will be slow in case of many files*/
	  join @points vs on mf.physical_name like vs.volume_mount_point+'%'
	  group by vs.volume_mount_point)
select 
	 d.[volume_mount_point]
	,d.[file_system_type]
	,d.[logical_volume_name]
	,d.[Total_Size_GB]
	,d.[Available_Size_GB]
	,d.[Space_Free_Percent]
	,d.[TypeAmount]
	,case when num_of_reads = 0 then 0 else (io_stall_read_ms/num_of_reads) end [ReadLatency]
	,case when io_stall_write_ms = 0 then 0 else (io_stall_write_ms/num_of_writes) end [WriteLatency]
	,case when (num_of_reads + num_of_writes = 0) then 0 else (io_stall/(num_of_reads+num_of_writes)) end [OverallLatency]
	,case when num_of_reads = 0 then 0 	else (num_of_bytes_read/num_of_reads/1024) end [AvgBytes_Read]
	,case when io_stall_write_ms = 0 then 0 else (num_of_bytes_written/num_of_writes/1024) end [AvgBytes_Write]
	,case when (num_of_reads + num_of_writes = 0) then 0 else ((num_of_bytes_read+num_of_bytes_written)/(num_of_reads+num_of_writes)/1024) end [AvgBytes_Transfer]
from data2 d
left join tab t on t.Drive=d.[volume_mount_point]

order by d.[volume_mount_point]
option (recompile)




</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
