      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>with 
	data as 
	(
	select 
		db_name(database_id) [db]
		,sum(case when type_desc='ROWS' then size else 0 end) [datasize]
		,sum(case when type_desc='LOG' then size else 0 end) [logsize]
		,convert(int,1.*sum(case when type_desc='LOG' then size else 0 end)/sum(case when type_desc='ROWS' then size else 0 end)*100) [percent]
	from sys.master_files 
	group by database_id
	having 	sum(case when type_desc='LOG' then size else 0 end) - sum(case when type_desc='ROWS' then size else 0 end)&gt;0
	and sum(size)&gt;12800 /*db size &gt;100MB*/
	)
,db_perc(num, dbs) as 
	(	select count(distinct database_id) 
			,(reverse(stuff(reverse((select distinct quotename(db_name(database_id)) + ', ' as 'data()' from sys.master_files t2 where is_percent_growth=1 and database_id&lt;&gt;1 and growth&gt;0 for xml path('') )),1,2,''))) 
		from sys.master_files 
		where is_percent_growth=1
		and database_id&lt;&gt;1
		and growth&gt;0 	)
,db_biglog (num, dbs) as    
	(
	select 
		count(1)
		,(reverse(stuff(reverse((select quotename([db])+': '+convert(varchar(5),[percent])+'%'+char(10) as 'data()' from data t2 order by [percent] desc for xml path('') )),1,1,''))) 
	from data
	)
, ttl as (
	select 
		count(1) [UserDatabases]
		, (select convert(numeric(18,1),sum(size/128)/1024.) [MB] from sys.master_files  where database_id&gt;4) [DBSizeGB]
		, sum(case when state_desc='ONLINE' then 1 else 0 end) [OnlineDBs]
		, sum(case when state_desc&lt;&gt;'ONLINE' then 1 else 0 end) [NotOnlineDBs]
		, sum(convert(smallint,is_auto_close_on)) [AutoClose]
		, sum(convert(smallint,is_auto_shrink_on)) [AutoShrink]
		, sum(convert(smallint,is_encrypted)) [EncryptedDBs]
		, sum(case when source_database_id is not NULL then 1 else 0 end) [Snapshots]
		, sum(convert(smallint,is_read_only)) [ReadOnly]
		, (select char(10)+'* '+name as [data()] from sys.databases where database_id&gt;4 order by name for xml path('')) [UserDbsList]
	from sys.databases 
	where database_id&gt;4 
)
select 
	t.[UserDatabases]
	,t.UserDbsList
	,t.[DBSizeGB]
	,t.[OnlineDBs]
	,t.[NotOnlineDBs]
	,t.[AutoClose]
	,t.[AutoShrink]
	,t.[EncryptedDBs]
	,t.[Snapshots]
	,t.[ReadOnly]
	, p.num [growthperc]
	, p.dbs [Gdbs]
	, b.num [biglog]
	, b.dbs [Bdbs]
from ttl t 
join db_perc p on 1=1
join db_biglog b on 1=1
option (recompile)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
